{
  "name": "react-native-svg-preview-panel",
  "displayName": "React Native SVG Preview Panel",
  "description": "Preview React Native SVG components directly in VS Code. Supports Circle, Rect, Path, LinearGradient, and more.",
  "version": "0.1.3",
  "publisher": "sinandsean",
  "author": {
    "name": "sinandsean"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sinandsean/react-native-svg-preview"
  },
  "bugs": {
    "url": "https://github.com/sinandsean/react-native-svg-preview/issues"
  },
  "homepage": "https://github.com/sinandsean/react-native-svg-preview#readme",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "keywords": [
    "react-native",
    "svg",
    "preview",
    "react-native-svg",
    "visualization"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCommand:rnSvgPreview.showPreview",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rnSvgPreview.showPreview",
        "title": "React Native SVG: Show Preview"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "rnSvgPreview.showPreview",
          "when": "editorLangId =~ /javascript|javascriptreact|typescript|typescriptreact/",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.27.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0"
  }
}
